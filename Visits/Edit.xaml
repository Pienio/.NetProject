<Window x:Class="Visits.Edit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Visits"
        mc:Ignorable="d"
        Title="Twój Profil" Height="547.196" Width="436.449">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="224*"/>
            <ColumnDefinition Width="207*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="label" Content="PESEL:" HorizontalAlignment="Left" Margin="32,10,0,0" VerticalAlignment="Top" Width="94"/>
        <Label x:Name="label2" Content="Imię:" HorizontalAlignment="Left" Margin="32,41,0,0" VerticalAlignment="Top" Width="94"/>
        <Label x:Name="label3" Content="Nazwisko:" HorizontalAlignment="Left" Margin="27,69,0,0" VerticalAlignment="Top" Width="94"/>
        <Label x:Name="label4" Content="Specjalizacja:" HorizontalAlignment="Left" Margin="32,100,0,0" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="Pes" HorizontalAlignment="Left" Height="23" Margin="126,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" Grid.ColumnSpan="2"/>
