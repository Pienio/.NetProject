        <TextBox x:Name="Nazw" HorizontalAlignment="Left" Height="23" Margin="126,72,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" Grid.ColumnSpan="2"/>
        <ComboBox x:Name="Spec" HorizontalAlignment="Left" Margin="126,100,0,0" VerticalAlignment="Top" Width="262" Grid.ColumnSpan="2"/>
        <Button x:Name="Reg" Content="Akceptuj" HorizontalAlignment="Left" Margin="32,0,0,9.6" Width="111" Height="40" VerticalAlignment="Bottom" Click="Reg_Click"/>
