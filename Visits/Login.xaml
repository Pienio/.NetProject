<Window x:Class="Visits.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Visits"
        mc:Ignorable="d"
        Title="Zaloguj" Height="188.255" Width="300">
    <Grid>
        <Label x:Name="label" Content="Pesel" HorizontalAlignment="Left" Margin="10,21,0,0" Width="60" Height="30" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Password" HorizontalAlignment="Left" Margin="10,80,0,0" Width="60" Height="26" VerticalAlignment="Top"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="75,80,0,0" VerticalAlignment="Top" Width="196" Height="26"/>
        <TextBox x:Name="usr" HorizontalAlignment="Left" Height="27" Margin="75,21,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="196"/>
        <Button x:Name="Log" Content="Zaloguj" HorizontalAlignment="Left" Margin="21,122,0,0" VerticalAlignment="Top" Width="91" Height="26" Click="Log_Click"/>
        <Button x:Name="Anul" Content="Anuluj" HorizontalAlignment="Left" Margin="176,122,0,0" VerticalAlignment="Top" Width="95" Height="26" Click="Anul_Click"/>

    </Grid>
</Window>
